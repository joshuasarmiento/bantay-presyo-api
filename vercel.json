{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PORT": 3000,
    "NODE_ENV": "production",
    "ALLOWED_ORIGINS": "https://bantaypresyo.vercel.app"
  }
}